{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/public/test.txt", "dest": "/public/test.txt"},
    { "src": "/public/images/snc_logo.png", "dest": "/public/images/snc_logo.png"},
    { "src": "/css/style.css", "dest": "/css/style.css" },
    { "src": "/js/script.js", "dest": "/js/script.js" },
    { "src": "/(.*)",  "dest": "/api/index.php" }
  ]
}

